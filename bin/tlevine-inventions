#!/usr/bin/env python
import sys

import tlevine

HRULE = '--------------------------------------\n'
def main():
    for url, description in tlevine.tlevine():
        if description:
            sys.stdout.write(HRULE + '%s\n\n%s\n\n' % (url, description))
    sys.stdout.write(HRULE)

if __name__ == '__main__':
    main()
